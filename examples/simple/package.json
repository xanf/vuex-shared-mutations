{
  "name": "vuex-shared-mutations-simple-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
